require File.expand_path(File.dirname(__FILE__) + '/../../spec_helper')

describe "/commits/gets" do
  before(:each) do
    render 'commits/gets'
  end
  
  it "should have a list of links to the individual commits" do
    response.should have_tag( 'li' ) do
      with_tag( 'ul' ) do
        with_tag( 'a' )
      end
    end
  end
end
